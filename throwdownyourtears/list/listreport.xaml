<Page x:Class="throwdownyourtears.list.listreport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:throwdownyourtears.list"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="listreport">

    <Grid>
        <Grid Margin="0,0,10,10">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Frame Background="LightSteelBlue" Grid.Row="1" Content="{Binding CurrentPage}" NavigationUIVisibility="Hidden"/>
            <Frame Background="White" Grid.Row="1" Content="{Binding CurrentPage}" NavigationUIVisibility="Hidden" Margin="10,77,10,10"/>
            <Button FontFamily="Segoe UI Black" FontWeight="UltraBlack" FontStyle="Normal" FontSize="16"  FlowDirection="LeftToRight" Background="LightPink" Content="Назад" Margin="10,10,634,385" Grid.Row="1" Click ="clickOpenmainwindow"/>
            <Button FontFamily="Segoe UI Black" FontWeight="UltraBlack" FontStyle="Normal" FontSize="16"  FlowDirection="LeftToRight" Background="LightPink" Content="Написать  поставщику" Margin="575,10,10,385" Grid.Row="1" Click="sendmessage"/>
        </Grid>
    </Grid>
</Page>